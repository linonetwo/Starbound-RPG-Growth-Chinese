{
  "name": "starbound-rpg-growth-chinese",
  "description": "Starbound Mod RPG Growth 汉化",
  "version": "0.1.0",
  "author": "linonetwo",
  "license": "UNLICENSED",
  "scripts": {
    "download:source": "rm -rf ./source && curl -LOk https://github.com/IcyVines/Starbound-RPG-Growth/archive/master.zip  && unzip ./master.zip && mv ./Starbound-RPG-Growth-master ./source && rm ./master.zip",
    "scan": "node ./scripts/index.js --scan",
    "generate:overwrite-missing": "node ./scripts/index.js --generate overwrite-missing",
    "pack:mac": "~/Library/'Application Support'/Steam/SteamApps/common/Starbound/osx/asset_packer ./translation ./RPGGrowthChinese.pak && rimraf ./dist && mkdir ./dist && cp ./RPGGrowthChinese.pak ./dist/contents.pak",
    "try": "node ./scripts/index.js --try",
    "generate:test": "node ./scripts/index.js --generate test",
    "unpack:mac": "~/Library/'Application Support'/Steam/SteamApps/common/Starbound/osx/asset_unpacker ./RPGGrowthChinese.pak ./translation.from.pak"
  },
  "dependencies": {
    "baidu-translate": "^1.1.0",
    "baidu-translate-api": "^0.4.3",
    "codegen.macro": "^4.0.0",
    "dotenv": "^8.2.0",
    "flow-runtime": "^0.17.0",
    "franc": "^5.0.0",
    "fs-jetpack": "^3.1.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "param.macro": "^3.2.1",
    "promise-retry": "^2.0.1",
    "strip-json-comments": "^3.1.1",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-flow": "^7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-array-last-index": "0.0.5",
    "babel-plugin-flow-runtime": "^0.19.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-macros": "^2.8.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.2"
  }
}
